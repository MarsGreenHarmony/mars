<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.himedia.sn.ecommerce5th.mars.greenharmony.seller.login.mapper.SellerLoginMapper">

	<select id="loginProc" parameterType="java.lang.String" resultType="SellerDto">
		SELECT
			SEQ_SLL
			, ID
			, CORP_NM
		FROM
			SELLER
		WHERE
			ID			= #{id}
	</select>
	
	
</mapper>